<h1>イベント作成</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@event) do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title %>

      <%= f.label :sub_title %>
      <%= f.text_field :sub_title %>

      <%= f.submit "作成", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
